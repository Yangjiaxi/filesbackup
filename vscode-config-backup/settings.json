{
    // ***************[Editor]***************
    // "editor.fontFamily": "FiraCode-Retina, Hei",
    // "editor.fontFamily": "Go Mono For Powerline, Hei",
    "editor.fontFamily": "Ligalex Mono, Hei",
    // "editor.fontFamily": "'Fixedsys Excelsior 3.01', Dotted Songti Square, Hack Nerd Font",
    // "editor.fontFamily": "Cascadia Code, Hei, Hack Nerd Font",
    // "editor.fontFamily": "JetBrains Mono, Hei",
    // "editor.fontFamily": "PragmataPro, Hei",
    // "editor.fontWeight": "300",
    "editor.renderLineHighlight": "all",
    "editor.snippetSuggestions": "top",
    "editor.suggestSelection": "first",
    "editor.minimap.enabled": false,
    "editor.fontLigatures": true,
    "editor.formatOnSave": true,
    "editor.wordWrap": "on",
    "editor.fontSize": 15,
    "editor.lineHeight": 18,
    "diffEditor.ignoreTrimWhitespace": true,
    "workbench.editor.scrollToSwitchTabs": true,
    "markdown.preview.fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe WPC', 'Segoe UI', 'Ubuntu', 'Droid Sans', sans-serif",
    // ***************[Terminal]***************
    // "terminal.integrated.fontFamily": "Cascadia Code, Hack Nerd Font, Hei",
    // "terminal.integrated.fontFamily": "Ligalex Mono, Hack Nerd Font, Hei",
    "terminal.integrated.fontFamily": "JetBrains Mono, Hack Nerd Font, Hei",
    // "terminal.integrated.fontFamily": "PragmataPro, Hack Nerd Font, Hei",
    "terminal.integrated.shell.osx": "/bin/zsh",
    "terminal.integrated.inheritEnv": true,
    "terminal.integrated.fontSize": 15,
    // ***************[Global AUX]***************
    "files.associations": {
        "*.y": "plaintext",
        "*.sh": "bat"
    },
    "workbench.startupEditor": "welcomePage",
    "http.proxy": "http://127.0.0.1:7890",
    "explorer.confirmDragAndDrop": false,
    "explorer.confirmDelete": false,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 30000,
    "http.proxySupport": "on",
    "git.confirmSync": false,
    "debug.console.fontFamily": "Ligalex Mono, Hei",
    "debug.console.fontSize": 14,
    // ***************[Theme]***************
    "window.title": "${activeEditorShort}${separator}${folderPath}",
    "material-icon-theme.folders.theme": "specific",
    "workbench.iconTheme": "vscode-icons",
    "workbench.colorTheme": "Solarized Light",
    "workbench.colorCustomizations": {
        "editor.findMatchBorder": "#ff0000",
        "editor.findMatchHighlightBorder": "#ffff00"
    },
    // ***************[C/C++]***************
    "C_Cpp.clang_format_style": "{ IndentWidth: 4, ColumnLimit: 100, IndentCaseLabels: true, AlignAfterOpenBracket: AlwaysBreak, AllowShortFunctionsOnASingleLine: false, BinPackArguments: false, BinPackParameters: false, AlignEscapedNewlines: Left, AllowShortLoopsOnASingleLine: true, AlignAfterOpenBracket: true}",
    "cmake.cmakePath": "/Applications/CMake.app/Contents/bin/cmake",
    "cmake.configureOnOpen": true,
    "cmake.debugConfig": {
        "stopAtEntry": false,
        "MIMode": "lldb",
        "miDebuggerPath": "/Users/yang/.vscode/extensions/ms-vscode.cpptools-0.28.3/debugAdapters/lldb-mi/bin/lldb-mi",
        "logging": {
            "trace": false,
            "engineLogging": false,
            "traceResponse": false,
            "moduleLoad": false
        }
    },
    "C_Cpp.updateChannel": "Insiders",
    // ***************[Python]***************
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--line-length=100"
    ],
    // ***************[JavaScript]*************** 
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[javascript]": {
        "editor.tabSize": 2,
    },
    "javascript.preferences.quoteStyle": "single",
    "javascript.format.enable": true,
    // ***************[TypeScript]*************** 
    "[typescript]": {
        "editor.tabSize": 2
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.preferences.quoteStyle": "single",
    // ***************[Rust]*************** 
    "rust.clippy_preference": "on",
    "rust-client.rustupPath": "/Users/yang/.cargo/bin/rustup",
    // ***************[Markdown]***************
    "markdown.preview.doubleClickToSwitchToEditor": false,
    "[markdown]": {
        "editor.wordWrap": "on",
        "editor.quickSuggestions": true
    },
    // ***************[LaTeX]***************
    "latex-workshop.view.pdf.viewer": "tab",
    "wordcount_cjk.activateLanguages": [
        "markdown",
        "plaintext",
        "latex"
    ],
    // ***************[Plugins]***************
    "liveServer.settings.donotShowInfoMsg": true,
    "liveServer.settings.donotVerifyTags": true,
    "npm.packageManager": "yarn",
    "python.jediEnabled": false,
    "[shellscript]": {
        "editor.defaultFormatter": "shakram02.bash-beautify"
    },
    "code-runner.executorMap": {
        "cpp": "cd $dir && g++ -std=c++17 $fileName -o $fileNameWithoutExt.out && $dir$fileNameWithoutExt.out && rm $dir$fileNameWithoutExt.out"
    },
    "java.configuration.checkProjectSettingsExclusions": false,
    "[python]": {
        "editor.defaultFormatter": "ms-python.python"
    },
    "java.semanticHighlighting.enabled": false,
    "window.zoomLevel": 0,
    "terminal.integrated.rendererType": "dom",
    "python.languageServer": "Microsoft",
    "python.defaultInterpreterPath": "ipython",
}