{
    // ***************[Editor]***************
    // "editor.fontFamily": "FiraCode-Retina, Hei",
    // "editor.fontFamily": "Fira Code, Hei",
    // "editor.fontFamily": "Go Mono For Powerline, Hei",
    // "editor.fontFamily": "PT Mono, Hei",
    // "editor.fontFamily": "Roboto Mono for Powerline, Hei",
    "editor.fontFamily": "Ligalex Mono, Hei",
    "editor.fontSize": 16,
    "editor.lineHeight": 21,
    "editor.formatOnSave": true,
    "editor.fontLigatures": true,
    "editor.snippetSuggestions": "top",
    "editor.minimap.renderCharacters": false,
    "editor.wordWrap": "off",
    "editor.renderControlCharacters": false,
    "editor.minimap.enabled": true,
    "editor.renderLineHighlight": "all",
    "editor.cursorWidth": 2,
    "editor.suggestSelection": "first",
    // ***************[Terminal]***************
    "terminal.integrated.fontFamily": "Ligalex Mono, Hack Nerd Font, Hei",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.shell.osx": "/bin/zsh",
    // ***************[Global AUX]***************
    "window.zoomLevel": 1,
    "explorer.confirmDelete": false,
    "files.autoSave": "afterDelay",
    "files.autoSaveDelay": 30000,
    "git.confirmSync": false,
    "workbench.startupEditor": "welcomePage",
    "explorer.confirmDragAndDrop": false,
    "breadcrumbs.enabled": true,
    // ***************[Theme]***************
    "window.title": "${activeEditorShort}${separator}${folderPath}",
    "material-icon-theme.folders.theme": "specific",
    "workbench.iconTheme": "material-icon-theme",
    "workbench.colorTheme": "Solarized Light",
    // ***************[C/C++]***************
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Chromium, BreakBeforeBraces: Allman, IndentWidth: 4, ColumnLimit: 80}",
    "cmake.cmakePath": "/Applications/CMake.app/Contents/bin/cmake",
    "cmake.configureOnOpen": true,
    "cmake.debugConfig": {
        "stopAtEntry": false,
        "MIMode": "lldb",
        "miDebuggerPath": "/Users/yang/.vscode/extensions/ms-vscode.cpptools-0.24.1/debugAdapters/lldb/bin/lldb-mi",
        "logging": {
            "trace": false,
            "engineLogging": false,
            "traceResponse": false,
            "moduleLoad": false
        }
    },
    "C_Cpp.updateChannel": "Insiders",
    // ***************[Python]***************
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--line-length=80"
    ],
    "python.linting.pep8Args": [
        "--ignore=E501"
    ],
    "python.pythonPath": "ipython",
    "python.jediEnabled": false,
    // ***************[JavaScript]*************** 
    "javascript.updateImportsOnFileMove.enabled": "always",
    "[javascript]": {
        "editor.tabSize": 2,
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "javascript.preferences.quoteStyle": "single",
    // ***************[TypeScript]*************** 
    "[typescript]": {
        "editor.tabSize": 2
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "typescript.preferences.quoteStyle": "single",
    // ***************[Rust]*************** 
    "rust.clippy_preference": "on",
    "rust-client.rustupPath": "/Users/yang/.cargo/bin/rustup",
    // ***************[Markdown]***************
    "markdown.preview.doubleClickToSwitchToEditor": false,
    // ***************[LaTeX]***************
    "latex-workshop.view.pdf.viewer": "tab",
    // ***************[Plugins]***************
    "liveServer.settings.donotShowInfoMsg": true,
    "npm.packageManager": "yarn",
    "liveServer.settings.donotVerifyTags": true,
    "http.proxySupport": "off",
    "leetcode.endpoint": "leetcode-cn",
    "leetcode.workspaceFolder": "/Users/yang/Desktop/Code/leetcode",
    "leetcode.hint.configWebviewMarkdown": false
}